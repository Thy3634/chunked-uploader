{
  "name": "@thy3634/chunked-uploader",
  "version": "0.0.3",
  "description": "Split file into chunks and upload. resumable, retriable, offline aware. Works on node, browser, and workers.",
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev": "npx --yes listhen -w --open ./app.ts",
    "lint": "eslint .",
    "lint:fix": "automd && eslint . --fix",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish  --access public && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "keywords": [
    "chunked-upload",
    "upload",
    "worker"
  ],
  "type": "module",
  "author": "thy3634",
  "license": "MIT",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/Thy3634/chunked-uploader.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "hash-wasm": "^4.11.0",
    "ofetch": "^1.3.4",
    "p-limit": "^6.1.0"
  },
  "devDependencies": {
    "vite": "^5.4.7",
    "@types/node": "^22.1.0",
    "@vitest/coverage-v8": "^2.0.5",
    "automd": "^0.3.8",
    "changelogen": "^0.5.5",
    "eslint": "^9.8.0",
    "eslint-config-unjs": "^0.3.2",
    "h3": "^1.12.0",
    "listhen": "^1.7.2",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "ufo": "^1.5.4",
    "unbuild": "^3.0.0-rc.7",
    "unstorage": "^1.12.0",
    "vitest": "^2.0.5"
  },
  "packageManager": "pnpm@9.7.0"
}